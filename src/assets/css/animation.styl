@keyframes fastspin {
  0% {
    transform: translateX(-50%) scale3d(0.5, 0.5, 0.5) rotateX(0deg) rotateY(0deg) rotateZ(0deg);
  }

  5%, 25% {
    transform: translateX(-50%) scale3d(0.5, 0.5, 0.5) rotateX(0deg) rotateY(90deg) rotateZ(0deg);
  }
  30%, 50% {
    transform: translateX(-50%) scale3d(0.5, 0.5, 0.5) rotateX(0deg) rotateY(180deg) rotateZ(0deg);
  }
  55%, 75% {
    transform: translateX(-50%) scale3d(0.5, 0.5, 0.5) rotateX(0deg) rotateY(270deg) rotateZ(0deg);
  }
  80%, 100% {
    transform: translateX(-50%) scale3d(0.5, 0.5, 0.5) rotateX(0deg) rotateY(360deg) rotateZ(0deg);
  }
}

@keyframes slowspin {
  0% {
    transform: translateX(-50%) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg);
  }

  5%, 25% {
    transform: translateX(-50%) scale3d(1, 1, 1) rotateX(0deg) rotateY(-90deg) rotateZ(0deg);
  }
  30%, 50%  {
    transform: translateX(-50%) scale3d(1, 1, 1) rotateX(0deg) rotateY(-180deg) rotateZ(0deg);
  }
  55%, 75% {
    transform: translateX(-50%) scale3d(1, 1, 1) rotateX(0deg) rotateY(-270deg) rotateZ(0deg);
  }
  80%, 100%  {
    transform: translateX(-50%) scale3d(1, 1, 1) rotateX(0deg) rotateY(-360deg) rotateZ(0deg);
  }
}
